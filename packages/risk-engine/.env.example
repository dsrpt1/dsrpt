# DSRPT Risk Engine Environment Variables
# Copy this file to .env and fill in your values

# Required: Keeper wallet private key (with 0x prefix)
# This wallet must be set as keeper on the contracts
PRIVATE_KEY=0xYOUR_PRIVATE_KEY_HERE

# Optional: Override RPC URL (default: https://mainnet.base.org)
RPC_URL=https://mainnet.base.org

# Optional: Log level (DEBUG, INFO, WARNING, ERROR)
LOG_LEVEL=INFO
